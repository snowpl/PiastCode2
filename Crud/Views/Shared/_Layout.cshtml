                        </button>
                        <button class="btn btn-default" type="button">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </button>
                    </span>
                </div><!-- /input-group -->
            </div>
        </div>
        @RenderBody()
    </div>








    <!-- Latest compiled and minified JavaScript -->
